##################################################################

@namespace "Name"

#_________________________________________________________________
BEGIN{ ###########################################################

	ptr		= _::def( "Name", tempFile::ptr, "{", "}" )

}#________________________________________________________________
func	def( n, pp, px, sx ,p ) { ##################################

	print "\t\t" API ": in"

	_::api()

	p = @API( n, pp, px, sx )

	print "\t\t" "Name: def: out"

	"" in _FILENAME

	return p }

#_____________________________________________________
func	new( pp, p ) { #################################

	print "\t\t" API ": in"

	_::api()

	p = @API( pp, p )

	print "\t\t" "Name: new: out"

	_FILENAME[ p ] = p

	return p }

#_____________________________________________________
func	del( p ) { #####################################

	print "\t\t" API ": in"

	delete _FILENAME[ p ]

	if ( _::api() ) {

		p = @API( p )

		print "\t\t" "Name: del: out"

		return p } }

