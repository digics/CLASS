func	_new( pp, p ) { ######################################

	print "\t" API ": IN"

	if ( _::api() )

		p = @API( pp, p )

	_FILENAME[ p ] = p

	return p }

#___________________________________________________________
func	_del( p ) { ##########################################

	print "\t" API ": IN"

	delete _FILENAME[ p ]

	if ( _::api() )

		return @API( p ) }
