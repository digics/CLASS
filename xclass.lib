
# base class upgrading example

@namespace "xclass"

BEGIN{

	PARENTPTR[ "" ]		= _::def( "xclass", "" ) }



func	_def( n, pp, px, sx ,p ) {				# classname::def

	p = ppapi4( n, pp, px, sx )
 
	print "xclass def method !!!!!!!!!!!!!!!"

	hid::get( CLASS[ p ], p )				# def method extension

	return p }

#_________________________________________________________________
##################################################################




func	ppapi4( p1,p2,p3,p4,p5,p6,p7,p8 ) {

	_::api()

	return @API( p1,p2,p3,p4 ) }




